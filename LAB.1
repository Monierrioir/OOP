//sort the numbe rin array in ascending 
#include <iostream>

int main() {
    using namespace std;
    int arr[4];
    int temp;

    // Input array elements
    for (int i = 0; i < 4; i++) {
        cin >> arr[i];
    }

    // Bubble Sort algorithm
    for (int i = 0; i < 4; i++) {
        for (int j = 0; j < 3; j++) {
            if (arr[j] > arr[j + 1]) {
                // Swap elements if they are in the wrong order
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    // Display the sorted array
    cout << "The ascending order elements: ";
    for (int a = 0; a < 4; a++) {
        cout << arr[a] << " ";
    }
    cout << endl;

    return 0;
}
